ttp({\n            url: `${!this.ruleForm.id ? `users/save` : `users/update`}`,\n            method: \"post\",\n            data: this.ruleForm\n          }).then(({ data }) => {\n            if (data && data.code === 0) {\n              this.$message({\n                message: \"操作成功\",\n                type: \"success\",\n                duration: 1500,\n                onClose: () => {\n                  this.parent.showFlag = false;\n                  this.parent.search();\n                }\n              });\n            } else {\n              this.$message.error(data.msg);\n            }\n          });\n        }\n      });\n    },\n    // 返回\n    back() {\n      this.parent.showFlag = false;\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n.editor {\n  height: 500px;\n}\n</style>\n"],"sourceRoot":"src/views/modules/users"}]}