CA,YAAA,IAAA,IAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,IAAA,CAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OATA;AAWA,KAzFA;AA0FA,IAAA,IA1FA,gBA0FA,EA1FA,EA0FA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,+BAAA,EAAA,CADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAGA,IAHA,CAGA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,IAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OATA;AAUA,KArGA;AAsGA,IAAA,QAtGA,oBAsGA,IAtGA,EAsGA;AACA,MAAA,MAAA,CAAA,IAAA,WAAA,IAAA;AACA,KAxGA;AAyGA,IAAA,eAzGA,6BAyGA;AACA,UAAA,SAAA,GAAA,WAAA;AACA,UAAA,SAAA,GAAA,KAAA,QAAA,CAAA,QAAA,GAAA,SAAA;AACA,MAAA,SAAA,CAAA,SAAA,CAAA,SAAA,EAAA,SAAA;AACA,KA7GA;AA8GA;AACA,IAAA,IA/GA,kBA+GA;AACA,WAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;AACA;AAlHA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n\r\n\r\n    <!-- 简历显示区域 -->\r\n    <div id=\"pdfResume\" class=\"resume-container\">\r\n      <h1>简历</h1>\r\n\r\n\r\n      <el-row>\r\n        <el-col :span=\"24\" class=\"photo-container user-photo-right\" >\r\n          <img :src=\"ruleForm.photo\" alt=\"User Photo\" class=\"user-photo\" />\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <p><strong>用户名：</strong>{{ ruleForm.username }}</p>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <p><strong>姓名：</strong>{{ ruleForm.fullname }}</p>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-row>\r\n        <el-col :span=\"12\">\r\n          <p><strong>性别：</strong>{{ ruleForm.gender }}</p>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <p><strong>电话：</strong>{{ ruleForm.phone }}</p>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-row>\r\n        <el-col :span=\"12\">\r\n          <p><strong>学历：</strong>{{ ruleForm.education }}</p>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <p><strong>岗位类别：</strong>{{ ruleForm.jobcategory }}</p>\r\n        </el-col>\r\n      </el-row>\r\n\r\n\r\n      <el-row>\r\n        <el-col :span=\"24\">\r\n          <p><strong>工作经历：</strong>{{ ruleForm.workexperience }}</p>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <el-row>\r\n        <el-col :span=\"24\">\r\n          <p><strong>个人基本情况：</strong></p>\r\n          <div v-html=\"ruleForm.personalinfo\"></div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n    <!-- 导出PDF按钮 -->\r\n    <el-button type=\"primary\" @click=\"exportResumePdf\">导出PDF简历</el-button>\r\n    <el-button @click=\"back()\">返回</el-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport pdfExport from \"../jobseekerinfo/export-pdf\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ruleForm: {\r\n        username: '',\r\n        fullname: '',\r\n        gender: '',\r\n        phone: '',\r\n        photo: '',\r\n        education: '',\r\n        jobcategory: '',\r\n        resume: '',\r\n        workexperience: '',\r\n        personalinfo: '',\r\n      },\r\n    };\r\n  },\r\n  props: [\"parent\"],\r\n  methods: {\r\n    init(id, type) {\r\n      if (id) {\r\n        this.id = id;\r\n        this.type = type;\r\n      }\r\n      if (this.type == 'info' || this.type == 'else') {\r\n        this.info(id);\r\n      } else if (this.type == 'cross') {\r\n        var obj = this.$storage.getObj('crossObj');\r\n        for (var o in obj) {\r\n          if (o == 'username') {\r\n            this.ruleForm.username = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'fullname') {\r\n            this.ruleForm.fullname = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'gender') {\r\n            this.ruleForm.gender = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'phone') {\r\n            this.ruleForm.phone = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'photo') {\r\n            this.ruleForm.photo = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'education') {\r\n            this.ruleForm.education = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'jobcategory') {\r\n            this.ruleForm.jobcategory = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'resume') {\r\n            this.ruleForm.resume = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'workexperience') {\r\n            this.ruleForm.workexperience = obj[o];\r\n            continue;\r\n          }\r\n          if (o == 'personalinfo') {\r\n            this.ruleForm.personalinfo = obj[o];\r\n            continue;\r\n          }\r\n        }\r\n      }\r\n      this.$http({\r\n        url: `${this.$storage.get('sessionTable')}/session`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          var json = data.data;\r\n          this.ruleForm.username = json.username\r\n          this.ruleForm.fullname = json.fullname\r\n          this.ruleForm.gender = json.gender\r\n          this.ruleForm.phone = json.phone\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n      this.$http({\r\n        url: `option/educationinfo/education`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.xueliOptions = data.data;\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n\r\n      this.$http({\r\n        url: `option/job_category/jobcategory`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.jobcategoryOptions = data.data;\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n\r\n    },\r\n    info(id) {\r\n      this.$http({\r\n        url: `jobseekerinfo/info/${id}`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.ruleForm = data.data;\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n    },\r\n    download(file) {\r\n      window.open(`${file}`)\r\n    },\r\n    exportResumePdf() {\r\n      const elementId = \"pdfResume\";\r\n      const htmlTitle = this.ruleForm.fullname+\"的简历.pdf\";\r\n      pdfExport.exportPdf(elementId, htmlTitle);\r\n    },\r\n    // 返回\r\n    back() {\r\n      this.parent.showFlag = true;\r\n      this.parent.ExportresumeFlag = false;\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.resume-container {\r\n  font-family: Arial, sans-serif;\r\n  line-height: 1.5;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  margin-top: 50px;\r\n   background-color: #f5f5f5;\r\n}\r\n\r\nh1 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\np {\r\n  margin: 5px 0;\r\n}\r\n\r\n.el-row {\r\n  margin-bottom: 10px;\r\n}\r\n\r\nimg {\r\n  margin-right: 20px;\r\n  border-radius: 4px;\r\n  border: 1px solid #ddd;\r\n}\r\n.user-photo-right {\r\n  position: absolute; /* 相对于父容器进行定位 */\r\n  top: -8px; /* 距离顶部20px */\r\n  right: 100px; /* 距离右侧20px */\r\n  width: 10px;\r\n  height: auto;\r\n  border-radius: 4px;\r\n  border: 1px solid #ddd;\r\n}\r\n.user-photo {\r\n  width: 100px; /* 调整图片的大小 */\r\n  height: 120px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/modules/jobseekerinfo"}]}