A;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAGA,IAHA,CAGA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,IAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,kBAAA,GAAA,IAAA,CAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OATA;AAWA,KAzFA;AA0FA,IAAA,IA1FA,gBA0FA,EA1FA,EA0FA;AAAA;;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,+BAAA,EAAA,CADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAGA,IAHA,CAGA,iBAAA;AAAA,YAAA,IAAA,SAAA,IAAA;;AACA,YAAA,IAAA,IAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,OATA;AAUA,KArGA;AAsGA,IAAA,QAtGA,oBAsGA,IAtGA,EAsGA;AACA,MAAA,MAAA,CAAA,IAAA,WAAA,IAAA;AACA,KAxGA;AAyGA,IAAA,eAzGA,6BAyGA;AACA,UAAA,SAAA,GAAA,WAAA;AACA,UAAA,SAAA,GAAA,KAAA,QAAA,CAAA,QAAA,GAAA,SAAA;AACA,MAAA,SAAA,CAAA,SAAA,CAAA,SAAA,EAAA,SAAA;AACA,KA7GA;AA8GA;AACA,IAAA,IA/GA,kBA+GA;AACA,WAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,WAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;AACA;AAlHA;AAlBA,CAAA","sourcesContent":["<template>\n  <div>\n\n\n    <!-- 简历显示区域 -->\n    <div id=\"pdfResume\" class=\"resume-container\">\n      <h1>简历</h1>\n\n\n      <el-row>\n        <el-col :span=\"24\" class=\"photo-container user-photo-right\" >\n          <img :src=\"ruleForm.photo\" alt=\"User Photo\" class=\"user-photo\" />\n        </el-col>\n        <el-col :span=\"12\">\n          <p><strong>用户名：</strong>{{ ruleForm.username }}</p>\n        </el-col>\n        <el-col :span=\"12\">\n          <p><strong>姓名：</strong>{{ ruleForm.fullname }}</p>\n        </el-col>\n      </el-row>\n\n      <el-row>\n        <el-col :span=\"12\">\n          <p><strong>性别：</strong>{{ ruleForm.gender }}</p>\n        </el-col>\n        <el-col :span=\"12\">\n          <p><strong>电话：</strong>{{ ruleForm.phone }}</p>\n        </el-col>\n      </el-row>\n\n      <el-row>\n        <el-col :span=\"12\">\n          <p><strong>学历：</strong>{{ ruleForm.education }}</p>\n        </el-col>\n        <el-col :span=\"12\">\n          <p><strong>岗位类别：</strong>{{ ruleForm.jobcategory }}</p>\n        </el-col>\n      </el-row>\n\n\n      <el-row>\n        <el-col :span=\"24\">\n          <p><strong>工作经历：</strong>{{ ruleForm.workexperience }}</p>\n        </el-col>\n      </el-row>\n\n      <el-row>\n        <el-col :span=\"24\">\n          <p><strong>个人基本情况：</strong></p>\n          <div v-html=\"ruleForm.personalinfo\"></div>\n        </el-col>\n      </el-row>\n    </div>\n    <!-- 导出PDF按钮 -->\n    <el-button type=\"primary\" @click=\"exportResumePdf\">导出PDF简历</el-button>\n    <el-button @click=\"back()\">返回</el-button>\n  </div>\n</template>\n\n<script>\nimport pdfExport from \"../jobseekerinfo/export-pdf\";\n\nexport default {\n  data() {\n    return {\n      ruleForm: {\n        username: '',\n        fullname: '',\n        gender: '',\n        phone: '',\n        photo: '',\n        education: '',\n        jobcategory: '',\n        resume: '',\n        workexperience: '',\n        personalinfo: '',\n      },\n    };\n  },\n  props: [\"parent\"],\n  methods: {\n    init(id, type) {\n      if (id) {\n        this.id = id;\n        this.type = type;\n      }\n      if (this.type == 'info' || this.type == 'else') {\n        this.info(id);\n      } else if (this.type == 'cross') {\n        var obj = this.$storage.getObj('crossObj');\n        for (var o in obj) {\n          if (o == 'username') {\n            this.ruleForm.username = obj[o];\n            continue;\n          }\n          if (o == 'fullname') {\n            this.ruleForm.fullname = obj[o];\n            continue;\n          }\n          if (o == 'gender') {\n            this.ruleForm.gender = obj[o];\n            continue;\n          }\n          if (o == 'phone') {\n            this.ruleForm.phone = obj[o];\n            continue;\n          }\n          if (o == 'photo') {\n            this.ruleForm.photo = obj[o];\n            continue;\n          }\n          if (o == 'education') {\n            this.ruleForm.education = obj[o];\n            continue;\n          }\n          if (o == 'jobcategory') {\n            this.ruleForm.jobcategory = obj[o];\n            continue;\n          }\n          if (o == 'resume') {\n            this.ruleForm.resume = obj[o];\n            continue;\n          }\n          if (o == 'workexperience') {\n            this.ruleForm.workexperience = obj[o];\n            continue;\n          }\n          if (o == 'personalinfo') {\n            this.ruleForm.personalinfo = obj[o];\n            continue;\n          }\n        }\n      }\n      this.$http({\n        url: `${this.$storage.get('sessionTable')}/session`,\n        method: \"get\"\n      }).then(({ data }) => {\n        if (data && data.code === 0) {\n          var json = data.data;\n          this.ruleForm.username = json.username\n          this.ruleForm.fullname = json.fullname\n          this.ruleForm.gender = json.gender\n          this.ruleForm.phone = json.phone\n        } else {\n          this.$message.error(data.msg);\n        }\n      });\n      this.$http({\n        url: `option/educationinfo/education`,\n        method: \"get\"\n      }).then(({ data }) => {\n        if (data && data.code === 0) {\n          this.xueliOptions = data.data;\n        } else {\n          this.$message.error(data.msg);\n        }\n      });\n\n      this.$http({\n        url: `option/job_category/jobcategory`,\n        method: \"get\"\n      }).then(({ data }) => {\n        if (data && data.code === 0) {\n          this.jobcategoryOptions = data.data;\n        } else {\n          this.$message.error(data.msg);\n        }\n      });\n\n    },\n    info(id) {\n      this.$http({\n        url: `jobseekerinfo/info/${id}`,\n        method: \"get\"\n      }).then(({ data }) => {\n        if (data && data.code === 0) {\n          this.ruleForm = data.data;\n        } else {\n          this.$message.error(data.msg);\n        }\n      });\n    },\n    download(file) {\n      window.open(`${file}`)\n    },\n    exportResumePdf() {\n      const elementId = \"pdfResume\";\n      const htmlTitle = this.ruleForm.fullname+\"的简历.pdf\";\n      pdfExport.exportPdf(elementId, htmlTitle);\n    },\n    // 返回\n    back() {\n      this.parent.showFlag = true;\n      this.parent.ExportresumeFlag = false;\n    },\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.resume-container {\n  font-family: Arial, sans-serif;\n  line-height: 1.5;\n  padding: 20px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  margin-top: 50px;\n  background-color: #f5f5f5;\n  width:800px;\n}\n\nh1 {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\np {\n  margin: 5px 0;\n}\n\n.el-row {\n  margin-bottom: 10px;\n}\n\nimg {\n  margin-right: 20px;\n  border-radius: 4px;\n  border: 1px solid #ddd;\n}\n.user-photo-right {\n  position: absolute; /* 相对于父容器进行定位 */\n  top: -8px; /* 距离顶部20px */\n  right: 100px; /* 距离右侧20px */\n  width: 10px;\n  height: auto;\n  border-radius: 4px;\n  border: 1px solid #ddd;\n}\n.user-photo {\n  width: 100px; /* 调整图片的大小 */\n  height: 120px;\n}\n</style>\n"],"sourceRoot":"src/views/modules/jobseekerinfo"}]}