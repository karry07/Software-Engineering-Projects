-if=\"ruleForm.interviewee\" label=\"面试者\" prop=\"interviewee\">\r\n              <el-input v-model=\"ruleForm.interviewee\" placeholder=\"面试者\" readonly></el-input>\r\n            </el-form-item>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item v-if=\"type != 'info'\" label=\"面试日期\" prop=\"interviewdate\">\r\n            <el-date-picker\r\n              v-model=\"ruleForm.interviewdate\"\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n              format=\"yyyy-MM-dd\"\r\n              value-format=\"yyyy-MM-dd\"\r\n              clearable>\r\n            </el-date-picker>\r\n          </el-form-item>\r\n          <div v-else>\r\n            <el-form-item v-if=\"ruleForm.interviewdate\" label=\"面试日期\" prop=\"interviewdate\">\r\n              <el-input v-model=\"ruleForm.interviewdate\" placeholder=\"面试日期\" readonly></el-input>\r\n            </el-form-item>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item v-if=\"type != 'info'\" label=\"职位\" prop=\"position\">\r\n            <el-input v-model=\"ruleForm.position\" placeholder=\"职位\" clearable></el-input>\r\n          </el-form-item>\r\n          <div v-else>\r\n            <el-form-item v-if=\"ruleForm.position\" label=\"职位\" prop=\"position\">\r\n              <el-input v-model=\"ruleForm.position\" placeholder=\"职位\" readonly></el-input>\r\n            </el-form-item>\r\n          </div>\r\n        </el-col>\r\n        <el-col :span=\"12\">\r\n          <el-form-item v-if=\"type != 'info'\" label=\"面试结果\" prop=\"result\">\r\n            <el-input v-model=\"ruleForm.result\" placeholder=\"面试结果\" clearable></el-input>\r\n          </el-form-item>\r\n          <div v-else>\r\n            <el-form-item v-if=\"ruleForm.result\" label=\"面试结果\" prop=\"result\">\r\n              <el-input v-model=\"ruleForm.result\" placeholder=\"面试结果\" readonly></el-input>\r\n            </el-form-item>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      <el-form-item>\r\n        <el-button v-if=\"type != 'info'\" type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button v-if=\"type != 'info'\" @click=\"back\">取消</el-button>\r\n        <el-button v-if=\"type == 'info'\" @click=\"back\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      id: '',\r\n      type: '',\r\n      ruleForm: {\r\n        companynumber: '',\r\n        companyname: '',\r\n        interviewer: '',\r\n        interviewee: '',\r\n        interviewdate: '',\r\n        position: '',\r\n        result: ''\r\n      },\r\n      rules: {\r\n        companynumber: [],\r\n        companyname: [],\r\n        interviewer: [],\r\n        interviewee: [],\r\n        interviewdate: [],\r\n        position: [],\r\n        result: []\r\n      }\r\n    };\r\n  },\r\n  props: [\"parent\"],\r\n  methods: {\r\n    init(id, type) {\r\n      this.id = id;\r\n      this.type = type;\r\n      if (this.id) {\r\n        this.fetchData();\r\n      }\r\n    },\r\n    fetchData() {\r\n      this.$http({\r\n        url: `interview/info/${this.id}`,\r\n        method: \"get\"\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.ruleForm = data.data;\r\n        }\r\n      });\r\n    },\r\n    onSubmit() {\r\n      this.$refs.ruleForm.validate(valid => {\r\n        if (valid) {\r\n          let url = this.id ? \"interview/update\" : \"interview/add\";\r\n          this.$http({\r\n            url: url,\r\n            method: \"post\",\r\n            data: this.ruleForm\r\n          }).then(({ data }) => {\r\n            if (data && data.code === 0) {\r\n              this.$message({\r\n                message: \"操作成功\",\r\n                type: \"success\",\r\n                duration: 1500,\r\n                onClose: () => {\r\n                  this.back();\r\n                  this.parent.getDataList();\r\n                }\r\n              });\r\n            } else {\r\n              this.$message.error(data.msg);\r\n            }\r\n          });\r\n        }\r\n      });\r\n    },\r\n    back() {\r\n      this.$emit('update:parent', { addOrUpdateFlag: false, showFlag: true });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.detail-form-content {\r\n  padding: 20px;\r\n}\r\n</style>\r\n"]}]}