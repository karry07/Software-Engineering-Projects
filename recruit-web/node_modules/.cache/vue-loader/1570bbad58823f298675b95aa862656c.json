-submenu>\n          <el-submenu\n              v-for=\" (menu,index) in item.backMenu\"\n              v-bind:key=\"menu.menu\"\n              :index=\"index+2+''\"\n          >\n            <template slot=\"title\">\n              <span>{{ menu.menu }}</span>\n            </template>\n            <el-menu-item\n                v-for=\" (child,sort) in menu.child\"\n                v-bind:key=\"sort\"\n                @click=\"menuHandler(child.tableName)\"\n                :index=\"(index+2)+'-'+sort\"\n            >{{ child.menu }}\n            </el-menu-item>\n          </el-submenu>\n        </el-menu>\n      </div>\n    </div>\n  </el-aside>\n</template>\n<script>\nimport menu from \"@/utils/menu\";\n\nexport default {\n  data() {\n    return {\n      menuList: [],\n      dynamicMenuRoutes: [],\n      role: \"\"\n    };\n  },\n  mounted() {\n    let menus = menu.list();\n    this.menuList = menus;\n    this.role = this.$storage.get(\"role\");\n  },\n  methods: {\n    menuHandler(name) {\n      this.$router.push({name: name});\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n</style>\n\n"]}]}