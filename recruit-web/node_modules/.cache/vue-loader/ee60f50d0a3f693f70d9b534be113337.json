lor: #0000002b;\r\n\t\t\t}\r\n\t\t\tdiv::-webkit-scrollbar {\r\n\t\t\t\twidth: 2px !important;\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n\t.el-dialog {\r\n\t\tmargin-top: 10vh !important;\r\n\t\twidth: 80% !important;\r\n\t\tfont-size: 25px;\r\n\t}\r\n\t::v-deep .el-dialog__header {\r\n\t\tdisplay: none;\r\n\t}\r\n}\r\n</style>"]}]}