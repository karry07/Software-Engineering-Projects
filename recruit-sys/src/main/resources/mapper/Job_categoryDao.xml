<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.Job_CategoryDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.Job_CategoryEntity" id="job_categoryMap">
        <result property="jobcategory" column="jobcategory"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.Job_CategoryVO" >
		SELECT * FROM job_category  job_category
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.Job_CategoryVO" >
		SELECT  job_category.* FROM job_category  job_category
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.Job_CategoryView" >

		SELECT  job_category.* FROM job_category  job_category
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.Job_CategoryView" >
		SELECT * FROM job_category  job_category <where> 1=1 ${ew.sqlSegment}</where>
	</select>
</mapper>